			[exec] (Libreoffice Impress) {libreoffice --impress}
			[exec] (Libreoffice math) {libreoffice --math}
			[exec] (Libreoffice Base) {libreoffice --base}
			
		[end]
		[submenu] (Internet)
			[exec] (Firefox) {firefox}
