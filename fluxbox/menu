[begin] (Fluxbox-1.3.7)
	[encoding] {UTF-8}
		[exec] (firefox) {firefox}
		[exec] (Thunderbird) {thunderbird}
		[submenu] (Shell)
			[exec] (White) {xterm -bg black -fg white}
			[exec] (Green) {xterm -bg black -fg green}
			[exec] (Red) {xterm -bg black -fg red}
			[exec] (Blue) {xterm -bg black -fg blue}
		[end]
		[separator]
		[submenu] (Acessórios)
			[exec] (Ranger) {xterm -e ranger}
			[exec] (Xcalc) {xcalc}
		[end]
		[submenu] (Editores)
			[exec] (Nano) {xterm -e nano}
			[exec] (Vim) {xterm -e vim}
			[exec] (Leafpad) {leafpad}
		[end]
		[submenu] (Educação)
			[exec] (Anki) {anki}
		[end]
		[submenu] (Office )
			[exec] (Libreoffice Writer) {libreoffice --writer}
