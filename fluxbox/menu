			[exec] (Nitrogen restore) {nitrogen --restore}
		[end]
		[submenu] (Video)
			[exec] (SimpleScreenRecorder) {simplescreenrecorder}
			[exec] (VLC) {vlc}
			[exec] (Smplayer) {smplayer}
			[exec] (MPV) {mpv}
		[end]
		[submenu] (Áudio)
			[exec] (Moc) {xterm -e mocp}
			[exec] (Audacious) {audacious}
			[exec] (Audacity) {audacity}
		[end]
		[submenu] (Sistema)
			[exec] (Xterm) {xterm}
			[exec] (Sakura) {sakura}
			[exec] (Lxappearence) {lxappearance}
			[exec] (VirtualBox) {virtualbox}
			[exec] (Nvidia Settings) {nvidia-settings}
		[end]
		[submenu] (Pentest)
			[submenu] (Cracker)
				[exec] (John) {xterm -e 'sudo john -h; zsh'}
				[exec] (Hashcat) {xterm -e 'hashcat -h; zsh'}
				[exec] (Hydra) {xterm -e 'hydra -h; zsh'}
			[end]
			[submenu] (Crypto)
				[exec] (FindMyHash) {xterm -e 'findmyhash -h; zsh'}
			[end]
			[submenu] (Exploitation)
				[exec] (Metasploit) {xterm -e msfconsole'}
			[end]
			[submenu] (Forensic)
				[exec] (Maltego) {maltego}
			[end]
			[submenu] (Fuzzer)
				[exec] (Burpsuit) {burpsuit}
			[end]
			[submenu] (Networking)
				[exec] (Tcpdump) {xterm -e 'sudo tcpdump -h; zsh'}
				[exec] (Hping) {xterm -e hping3}
			[end]
			[submenu] (Proxy)
				[exec] (Proxychains) {xterm -e 'proxychains; zsh'}
				[exec] (Tor) {xterm -e 'tor -h; zsh'}
			[end]
			[submenu] (Scanner)
				[exec] (Nmap) {xterm -e 'nmap -h; zsh'}
